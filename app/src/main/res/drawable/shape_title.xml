<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <stroke
        android:width="2sp"
        android:color="@color/colorTitleEnd"
    />
    <padding
        android:left="2sp"
        android:right="2sp"
        android:top="2sp"
        android:bottom="2sp"
    />
    <corners
        android:radius="8dp"
    />
    <solid
        android:color="@color/colorTitleStart"
    />
    <gradient
        android:angle="0"
        android:centerColor="@color/colorTitleCenter"
        android:startColor="@color/colorTitleStart"
        android:endColor="@color/colorTitleEnd"
        android:type="linear" />
</shape>